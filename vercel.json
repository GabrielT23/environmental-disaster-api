{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./src/main.ts"
      }
    ],
    "env": {
      "DATABASE_URL": "postgresql://username:password@host:port/database"
    }
  }
  